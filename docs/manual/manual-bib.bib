file = {:Users/suku924/Downloads/10.1016j.ijepes.2019.01.013.pdf:pdf},
issn = {01420615},
journal = {International Journal of Electrical Power and Energy Systems},
keywords = {Dynamic economic dispatch,Frequency stability,Optimization,Primary frequency response,Smart load},
number = {June},
pages = {240--251},
title = {{Look ahead dynamic security-constrained economic dispatch considering frequency stability and smart loads}},
volume = {108},
year = {2019}
}
